Users\\vitor\\Documents\\apps\\pessoal\\Christmas\\frontend\\src\\App.js",[],"C:\\Users\\vitor\\Documents\\apps\\pessoal\\Christmas\\frontend\\src\\pages\\Confirmar.jsx",["33"],"import React, { useEffect, useState } from \"react\";\r\nimport { useQuery } from \"react-query\";\r\nimport CardPedido from \"../components/CardPedido\";\r\nimport * as Fi from \"react-icons/fi\";\r\nimport api from \"../services/api\";\r\n\r\nimport cogo from \"cogo-toast\";\r\n\r\nfunction RealizeUmSonho() {\r\n  const [pedidos, setPedidos] = useState([]);\r\n  const [pedidoId, setPedidoId] = useState(\"\");\r\n  const [auth, setAuth] = useState(null);\r\n\r\n  const { isLoading, isFetching, error, refetch } = useQuery(\r\n    [\"pedidos\", pedidoId],\r\n    fetchPedidos\r\n  );\r\n\r\n  useEffect(() => {\r\n    const stored = localStorage.getItem(\"auth\");\r\n    if(!stored) return false\r\n\r\n    setAuth(stored);\r\n    api.defaults.headers.Authorization = stored;\r\n  });\r\n\r\n  async function handleLogin() {\r\n    const { hide } = cogo.loading(\"Fazendo login\");\r\n    const result = await api.post(\"/auth\", { password });\r\n    hide();\r\n\r\n    const { login } = result.data;\r\n    if (!login) {\r\n      return cogo.error('Senha incorreta...');\r\n    }\r\n    cogo.success('Login realizado com sucesso');\r\n    setAuth(login);\r\n    localStorage.setItem(\"auth\", login);\r\n    api.defaults.headers.Authorization = login;\r\n  }\r\n\r\n  const [password, setPassword] = useState(\"\");\r\n  if (!auth) {\r\n    return (\r\n      <div className=\"hover\">\r\n        <input\r\n          type=\"password\"\r\n          placeholder=\"Senha de acesso\"\r\n          id=\"login-input\"\r\n          onChange={(e) => setPassword(e.target.value)}\r\n          value={password}\r\n        />\r\n        <div id=\"login-btn\" onClick={handleLogin}>\r\n          <Fi.FiLogIn /> Login\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n\r\n  async function fetchPedidos() {\r\n    const query = pedidoId ? { pedidoId } : {};\r\n    console.log(query);\r\n    const result = await api.get(\"/pedido\", { params: query });\r\n    const pedidos = result.data;\r\n    setPedidos(pedidos);\r\n  }\r\n\r\n  return (\r\n    <div className=\"hover flex-start\">\r\n      <div className=\"title\">\r\n        <h3>Confirmação de pedidos</h3>\r\n        <p>Somente confirme o pedido se ele foi realizado!</p>\r\n        <input\r\n          placeholder=\"Número do pedido realizado...\"\r\n          className=\"form-control\"\r\n          onChange={(e) => setPedidoId(e.target.value)}\r\n          value={pedidoId}\r\n        />\r\n      </div>\r\n      {(isLoading || isFetching) && <h2>Carregando pedidos...</h2>}\r\n      {error && <h2>Erro ao carregar pedidos...</h2>}\r\n      {pedidos.length ? (\r\n        pedidos.map((pedido) => (\r\n          <CardPedido pedido={pedido} key={pedido.id} confirmar={true} afterChange={refetch} />\r\n        ))\r\n      ) : (\r\n        <CardPedido\r\n          pedido={{ status: 2, pedido: \"Nenhum pedido encontrado\" }}\r\n        />\r\n      )}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default RealizeUmSonho;\r\n","C:\\Users\\vitor\\Documents\\apps\\pessoal\\Christmas\\frontend\\src\\components\\CardPedido.jsx",["34"],"import React, { useEffect, useState } from \"react\";\r\nimport * as Fa from \"react-icons/fa\";\r\nimport api from \"../services/api\";\r\n\r\nfunction CardPedido({ pedido, confirmar, afterChange }) {\r\n  const [link, setLink] = useState(\"\");\r\n\r\n  useEffect(() => {\r\n    const number = \"5547999254473\";\r\n    const text = `Olá!! Quero realizar um sonho! Eu selecionei o pedido de número ${pedido.id}. Como faço para realizá-lo?`;\r\n    setLink(`https://api.whatsapp.com/send?phone=${number}&text=${text}`);\r\n  }, [pedido]);\r\n\r\n  async function handleConfirmar() {\r\n    await api.post(\"/pedido/setStatus\", { pedidoId: pedido.id, status: 1 });\r\n    if (afterChange) afterChange();\r\n  }\r\n  async function handleDesconfirmar() {\r\n    await api.post(\"/pedido/setStatus\", { pedidoId: pedido.id, status: 0 });\r\n    if (afterChange) afterChange();\r\n  }\r\n\r\n  return (\r\n    <div className={`card-pedido ${pedido.status ? \"resolvido\" : \"\"}`}>\r\n      <div className=\"card-header\">Pedido nº {pedido.id}</div>\r\n      <div className=\"card-body\">{pedido.pedido}</div>\r\n      <div className=\"card-footer\">\r\n        {!confirmar && (\r\n          <>\r\n            {pedido.status !== 2 ? (\r\n              <a\r\n                className={`btn-resolver-pedido ${\r\n                  pedido.status ? \"disabled\" : \"\"\r\n                }`}\r\n                href={!pedido.status ? link : \"#\"}\r\n                target={!pedido.status ? \"_blank\" : \"\"}\r\n              >\r\n                {pedido.status ? (\r\n                  \"Pedido realizado!\"\r\n                ) : (\r\n                  <>\r\n                    <Fa.FaWhatsapp /> Realizar!!\r\n                  </>\r\n                )}\r\n              </a>\r\n            ) : (\r\n              <center>\r\n                <br />\r\n                Tem certeza que digitou o número certo??\r\n              </center>\r\n            )}\r\n          </>\r\n        )}\r\n\r\n        {confirmar && !pedido.status && (\r\n          <button className=\"btn-resolver-pedido\" onClick={handleConfirmar}>\r\n            Confirmar realização\r\n          </button>\r\n        )}\r\n        {confirmar && !!pedido.status && pedido.status > 0 && (\r\n          <button\r\n            className=\"btn-resolver-pedido disabled\"\r\n            onClick={handleDesconfirmar}\r\n          >\r\n            Pedido já realizado\r\n          </button>\r\n        )}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default CardPedido;\r\n","C:\\Users\\vitor\\Documents\\apps\\pessoal\\Christmas\\frontend\\src\\index.js",[],"C:\\Users\\vitor\\Documents\\apps\\pessoal\\Christmas\\frontend\\src\\services\\api.js",[],"C:\\Users\\