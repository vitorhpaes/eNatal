 sorriso é um passo a mais por um mundo\r\n          melhor!\r\n          <br />\r\n          Vem comigo, vou te mostrar algum sonho que você pode realizar!\r\n        </p>\r\n        <input\r\n          placeholder=\"Busque o seu pedido pelo número!\"\r\n          className=\"form-control\"\r\n          onChange={(e) => setPedidoId(e.target.value)}\r\n          value={pedidoId}\r\n        />\r\n      </div>\r\n      {(isLoading || isFetching) && <h2>Carregando pedidos...</h2>}\r\n      {error && <h2>Erro ao carregar pedidos...</h2>}\r\n      {pedidos.length ? pedidos.map((pedido) => (\r\n        <CardPedido pedido={pedido} key={pedido.id} />\r\n      )) : <CardPedido pedido={{status: 2, pedido: \"Nenhum pedido encontrado\"}} />}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default RealizeUmSonho;\r\n","C:\\Users\\vitor\\Documents\\apps\\pessoal\\Christmas\\frontend\\src\\pages\\Pedido.jsx",["37"],"import React, {useState, useEffect} from \"react\";\r\nimport api from \"../services/api\";\r\n\r\nfunction Pedido() {\r\n  const [pedido, setPedido] = useState(\"\");\r\n  const [numeroPedido, setNumeroPedido] = useState(false);\r\n\r\n  useEffect(() => {\r\n    const numPedido = localStorage.getItem(\"numeroPedido\");\r\n    // setNumeroPedido(numPedido);\r\n  }, []);\r\n\r\n  function setNumeroPedidoLocal(num) {\r\n    localStorage.setItem(\"numeroPedido\", num);\r\n    setNumeroPedido(num);\r\n  }\r\n\r\n  async function enviarPedido() {\r\n    const response = await api.post(\"/pedido\", { pedido: pedido });\r\n    const { numero } = response.data;\r\n    setNumeroPedidoLocal(numero);\r\n  }\r\n\r\n  return (\r\n    <div className=\"hover\">\r\n      <div className=\"title\">\r\n        <h3>Natal solidário</h3>\r\n        <h1>Lages - SC</h1>\r\n      </div>\r\n      <div className=\"card-pedido\">\r\n        {!numeroPedido ? (\r\n          <>\r\n            <textarea\r\n              className=\"caixa\"\r\n              placeholder=\"Digite seu pedido aqui\"\r\n         